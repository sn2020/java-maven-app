pipeline {
  
  agent any {
    
    stages {
      
      stage ("build") {
        step {
          echo "Building from Jenkinsfile"
        }
      }  
      stage ("Test") {
        step {
          echo "Testing from Jenkinsfile"
        }
      }
      stage ("Deploy") {
        step {
          echo "Deploying from Jenkinsfile"
        }
      }
    }
  }
}
